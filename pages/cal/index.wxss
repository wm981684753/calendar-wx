view{
  color: #333;
}
.list{
  border: 1px solid rgb(201, 233, 230);
  width: 90%;
  /* height: 68rpx; */
  /* background-color: rgb(201, 233, 230); */
  margin: 30rpx auto;
  border-radius: 10rpx;
  padding: 10rpx;
  box-shadow:0px 2px 4px 2px #DDDDDD;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #333;
  line-height: 52rpx;
}

.tolist{
  border: 1px solid rgb(201, 233, 230);
  width: 90%;
  /* height: 68rpx; */
  background-color: rgb(229 234 234);
  margin: 30rpx auto;
  border-radius: 10rpx;
  padding: 10rpx;
  box-shadow:0px 2px 4px 2px #DDDDDD;
  color: #aaa;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 52rpx;
}

image{
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
  border-radius: 30rpx;
  vertical-align: bottom;
}